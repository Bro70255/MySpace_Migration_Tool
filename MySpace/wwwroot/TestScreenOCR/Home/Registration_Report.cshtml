@{
    ViewData["Title"] = "Registration Report";
}

<h2 class="mb-3">Registration Report</h2>

<div class="search-box mb-3">
    <input type="text" id="txtSearch" class="form-control"
           placeholder="Search..."
           onkeyup="Initialize_Registration_Report_Details();" />
</div>

<table class="table table-bordered table-striped">
    <thead class="table-dark">
        <tr>
            <th>Full Name</th>
            <th>Phone</th>
            <th>Email</th>
            <th>Address</th>
            <th>Place</th>
            <th>Pin Code</th>
        </tr>
    </thead>
    <tbody id="tdtable">
    </tbody>
</table>

<script>
    $(document).ready(function () {
        Initialize_Registration_Report_Details();
    });    
</script>
